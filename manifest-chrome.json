{
  "content_scripts": [
    {
      "js": ["remover.js"],
      "matches": ["https://www.google.com/*", "https://duckduckgo.com/*", "https://www.bing.com/*"]
    }
  ],
  "manifest_version": 3,
  "name": "Search Engine Ad Remover",
  "version": "2.1.0",
  "action": {
    "default_title": "Search Engine Ad Remover",
    "default_popup": "popup.html"
  }
}